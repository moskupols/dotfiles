#define ALL(v) (v).begin(),(v).end()
#define RALL(v) (v).rbegin(),(v).rend()
#define UNIQ(v) sort(ALL(v)),v.erase(unique(ALL(v)),(v).end())

typedef long long int64;
typedef long double real;

class $CLASSNAME$
{
public:
    $RETURNTYPE$ $METHODNAME$($METHODPARAMS$)
    {
        return $RETURNTYPE$();
    }
};
